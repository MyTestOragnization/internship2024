@model IEnumerable<Piosenka>

@{

}

<h1>
   
    @foreach(var song in Model)
    {
        @Html.Raw(song.SongLyrics.Replace("|", "<br />"));     
        @song.SongID <br>
        @song.SongTitle <br />
           
        <a asp-action="Edit" asp-route-SongID="@song.SongID">Edit</a>
        <a asp-action="Delete" asp-route-SongID="@song.SongID">Delete</a>
        <br /><br />
    }

</h1>